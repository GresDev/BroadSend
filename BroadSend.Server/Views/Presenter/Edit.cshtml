@model Presenter

<partial name="_PresenterTitle" />

<div>
    <h2 class="font-weight-light">@Model.Name</h2>
</div>

<partial name="_EditItem" />

@if (ViewBag.ErrorMessage != string.Empty)
{
    <partial name="_ErrorMessage" />
}
else
{
    <div>
        <form asp-controller="Presenter" asp-action="Edit">

            <partial name="_PresenterCard" />

        </form>
    </div>
}